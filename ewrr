	    driver.findElement(By.linkText("Application Name")).click();
	    // 6 | click | css=#R1702416828355680327_sort_widget_action_up > .a-Button |  | 
	    driver.findElement(By.cssSelector("#R1702416828355680327_sort_widget_action_up > .a-Button")).click();
