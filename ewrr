package testautomation.awsm;

import org.junit.Test;
import org.openqa.selenium.By;
import org.openqa.selenium.Dimension;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class ReportsPage_ApplicationAccessLogs {
	
	WebDriver driver;
	
	
	  @Test
	  public void reportsPageApplicationAccessLog() {
	    // Test name: ReportsPage_ApplicationAccessLog
	    // Step # | name | target | value | comment
	    // 1 | open | https://apexq.fdic.gov/apex/f?p=911:10010:3043243330642:::::&tz=-6:00 |  | 
