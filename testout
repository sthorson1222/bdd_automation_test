  @Test
  public void reportsPageSearchUsers() {
    driver.get("https://apexq.fdic.gov/apex/f?p=911:10010:16662279564433:::::&tz=-6:00");
    driver.manage().window().setSize(new Dimension(1936, 945));
    driver.findElement(By.id("B1156001730250484216")).click();
    driver.findElement(By.id("1_menubar_1i")).click();
    driver.findElement(By.cssSelector("#R1704391717264501094_tab b")).click();
    driver.findElement(By.xpath("//li[7]/a/span/b")).click();
    driver.findElement(By.id("R1704391717264501094_search_field")).click();
    driver.findElement(By.id("R1704391717264501094_search_field")).click();
    driver.findElement(By.id("R1704391717264501094_search_field")).sendKeys("sthorson");
    driver.findElement(By.id("R1704391717264501094_search_button")).click();
    driver.findElement(By.cssSelector("#\\31 629551212722353141_orig .u-tL:nth-child(1)")).click();
    assertThat(driver.findElement(By.cssSelector("#\\31 629551212722353141_orig .u-tL:nth-child(1)")).getText(), is("sthorson"));
    driver.findElement(By.cssSelector(".t-Body-contentInner")).click();
    driver.findElement(By.cssSelector(".a-IRR-controls-item--search .a-Button")).click();
    driver.findElement(By.linkText("Logout")).click();
    driver.close();
  }
  @Test
  public void reportsPageUserActivityLogs() {
    driver.get("https://apexq.fdic.gov/apex/f?p=911:10010:1611850248477:::::&tz=-6:00");
    driver.manage().window().setSize(new Dimension(1920, 945));
    driver.findElement(By.id("B1156001730250484216")).click();
    driver.findElement(By.id("1_menubar_1i")).click();
    driver.findElement(By.cssSelector("#R1702441628196793822_tab b")).click();
    driver.findElement(By.id("R1702441628196793822_column_search_root")).click();
    driver.findElement(By.id("R1702441628196793822_column_search_drop_2_c0i")).click();
    driver.findElement(By.id("R1702441628196793822_search_field")).sendKeys("awsm");
    driver.findElement(By.id("R1702441628196793822_search_button")).click();
    driver.findElement(By.id("R1702441628196793822_column_search_root")).click();
    driver.findElement(By.id("R1702441628196793822_column_search_drop_2_c1i")).click();
    driver.findElement(By.id("R1702441628196793822_search_field")).sendKeys("sthorson");
    driver.findElement(By.id("R1702441628196793822_search_button")).click();
    driver.findElement(By.id("R1702441628196793822_column_search_root")).click();
    driver.findElement(By.id("R1702441628196793822_column_search_drop_2_c3i")).click();
    driver.findElement(By.id("R1702441628196793822_search_field")).sendKeys("reports");
    driver.findElement(By.id("R1702441628196793822_search_button")).click();
    {
      WebElement element = driver.findElement(By.cssSelector(".a-IRR-controls-item--filter:nth-child(1) .a-Button"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element).clickAndHold().perform();
    }
    {
      WebElement element = driver.findElement(By.cssSelector(".a-IRR-controls-item--filter:nth-child(1) .a-Button"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element).perform();
    }
    {
      WebElement element = driver.findElement(By.cssSelector(".a-IRR-controls-item--filter:nth-child(1) .a-Button"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element).release().perform();
    }
    driver.findElement(By.cssSelector(".a-IRR-controls-item--filter:nth-child(1) .a-Button")).click();
    driver.findElement(By.cssSelector(".a-IRR-controls-item--filter:nth-child(1) .a-Button")).click();
    driver.findElement(By.cssSelector(".a-IRR-controls-item--filter .a-Button")).click();
  }
}
