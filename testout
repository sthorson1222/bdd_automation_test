  @Test
  public void homePage() {
    driver.get("https://apexq.fdic.gov/apex/f?p=911:10010:14913122144989:::::&tz=-6:00/apex/f?p=911:10010:14913122144989:::::&tz=-6:00");
    driver.manage().window().setSize(new Dimension(1552, 840));
    driver.findElement(By.id("B1156001730250484216")).click();
    driver.findElement(By.cssSelector("#R1698111923875969373 > .t-Region-header > .t-Region-headerItems--title")).click();
    driver.findElement(By.cssSelector(".t-Button--navBar")).click();
    driver.findElement(By.id("report_R1698111923875969373")).click();
    // username
    assertThat(driver.findElement(By.cssSelector(".t-Button--navBar")).getText(), is("sthorson"));
    driver.findElement(By.cssSelector("#R1698111923875969373 > .t-Region-header > .t-Region-headerItems--title")).click();
    assertThat(driver.findElement(By.cssSelector("#R1698111923875969373_heading > b")).getText(), is("Application & Active Users"));
    driver.findElement(By.cssSelector("#report_R1698131933823615602 tr:nth-child(1) > .t-Report-cell")).click();
    driver.findElement(By.cssSelector("#R1698131933823615602 .t-Region-headerItems--title")).click();
    assertThat(driver.findElement(By.cssSelector("#R1698131933823615602_heading > b")).getText(), is("Users created in last 30 days"));
    driver.findElement(By.cssSelector("#R1698131344081609159 .t-Region-headerItems--title")).click();
    driver.findElement(By.cssSelector("#R1698131344081609159 .t-Region-body")).click();
    assertThat(driver.findElement(By.cssSelector("#R1698131344081609159_heading > b")).getText(), is("Top Applications by Activity"));
    driver.findElement(By.cssSelector("#R1698131344081609159 .t-Region-headerItems--title")).click();
    driver.close();
  }
